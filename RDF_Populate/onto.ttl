###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Actor

:Actor rdf:type :Class ;
       
       rdfs:subClassOf :Person .



###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Director

:Director rdf:type :Class ;
          
          rdfs:subClassOf :Person .



###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Genre

:Genre rdf:type :Class ;
       
       :equivalentClass [ rdf:type :Class ;
                          :oneOf ( :Family
                                   :Sci-Fi
                                   :Animation
                                   :Biography
                                   :Musical
                                   :Reality-TV
                                   :Comedy
                                   :Thriller
                                   :Music
                                   :Film-Noir
                                   :Short
                                   :Game-Show
                                   :Talk-Show
                                   :Drama
                                   :Adventure
                                   :Adult
                                   :News
                                   :Horror
                                   :Action
                                   :Sport
                                   :Crime
                                   :History
                                   :Documentary
                                   :Romance
                                   :War
                                   :Mystery
                                   :Western
                                   :Fantasy
                                 )
                        ] .



###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Movie

:Movie rdf:type :Class ;
       
       rdfs:subClassOf [ rdf:type :Restriction ;
                         :onProperty :hasMovieDuration ;
                         :someValuesFrom xsd:int
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMovieDescription ;
                         :someValuesFrom xsd:string
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMovieName ;
                         :someValuesFrom xsd:string
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMovieLaunchDate ;
                         :someValuesFrom xsd:dateTime
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMovieClassification ;
                         :someValuesFrom xsd:double
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMovieId ;
                         :qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         :onDataRange xsd:string
                       ] ,
                       [ rdf:type :Restriction ;
                         :onProperty :hasMoviePoster ;
                         :someValuesFrom xsd:string
                       ] .



###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Person

:Person rdf:type :Class ;
        
        rdfs:subClassOf [ rdf:type :Restriction ;
                          :onProperty :hasPersonName ;
                          :someValuesFrom xsd:string
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasPersonBirthPlace ;
                          :someValuesFrom xsd:string
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasPersonId ;
                          :qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          :onDataRange xsd:string
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasPersonBirth ;
                          :someValuesFrom xsd:dateTime
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasPersonMiniBio ;
                          :someValuesFrom xsd:string
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasPersonPicture ;
                          :someValuesFrom xsd:string
                        ] .



###  http://www.movierecomendation.pt/ontology/movierecomendation.owl#Studio

:Studio rdf:type :Class ;
        
        rdfs:subClassOf [ rdf:type :Restriction ;
                          :onProperty :hasStudioId ;
                          :qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          :onDataRange xsd:string
                        ] ,
                        [ rdf:type :Restriction ;
                          :onProperty :hasStudioName ;
                          :someValuesFrom xsd:string
                        ] .





